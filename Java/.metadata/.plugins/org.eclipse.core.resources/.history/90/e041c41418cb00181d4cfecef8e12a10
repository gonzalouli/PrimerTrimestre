
public class VolCono {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		final double pi=Math.PI;
		double diametro=14.2;
		float altura=20;
		
		float resultado= (float)(pi*Math.pow(diametro/2,2)*altura)/3;
		
		System.out.println("El volumen es: "+ resultado+"cmÂ³");
		
	}

}
